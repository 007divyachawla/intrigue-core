<h3>Dossier</h3>

<p>People:</p>
<ul>
  <% @persons.each do |e| %>
  <li><a href="/v1/<%=@project_name%>/entities/<%=h e.id%>"><%= h "#{e.type_string} : #{e.name}"  %></a></li>
  <% end %>
</ul>

